    "eslint-plugin-prettier": "5.2.1",
    "globals": "15.13.0",
    "prettier": "3.4.2",
    "ts-node": "10.9.2",
    "turbo": "2.3.3",
    "typescript": "5.7.2",
    "typescript-eslint": "8.17.0",
