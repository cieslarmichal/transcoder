    "eslint-plugin-prettier": "5.2.1",
    "globals": "15.9.0",
    "prettier": "3.3.3",
    "ts-node": "10.9.2",
    "turbo": "2.1.2",
    "typescript": "5.6.2",
    "typescript-eslint": "8.5.0",
